{
  "name": "express-laravel-style",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "main": "index.js","scripts": {
    "dev": "ts-node --esm --project tsconfig.json -r tsconfig-paths/register src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "db:generate": "npx drizzle-kit generate --config=./src/database/drizzle.config.ts",
    "db:migrate": "npx drizzle-kit migrate --config=./src/database/drizzle.config.ts",
    "db:push": "npx drizzle-kit push --config=./src/database/drizzle.config.ts",
    "db:pull": "npx drizzle-kit pull --config=./src/database/drizzle.config.ts",
    "db:studio": "npx drizzle-kit studio --config=./src/database/drizzle.config.ts",
    "db:generate:custom": "npx drizzle-kit generate --custom --name --config=./src/database/drizzle.config.ts",
    "db:status": "npx drizzle-kit status --config=./src/database/drizzle.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "graphql": "^15.10.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-graphql": "^0.8.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/graphql": "^14.2.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.4.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "@types/nanoid": "^2.1.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "_moduleAliases": {
    "@user.controllers": "dist/src/modules/user/controllers",
    "@user.routes": "dist/src/modules/user/routes",
    "@admin.controllers": "dist/src/modules/admin/controllers",
    "@admin.routes": "dist/src/modules/admin/routes",
    "@config": "dist/src/core/config/",
    "@middleware": "dist/src/core/middleware/",
    "@migrations": "dist/src/infrastructure/database/migrations",
    "@seeds": "dist/src/infrastructure/database/seeds",
    "@tests": "dist/src/infrastructure/database/tests",
    "@monitoring": "dist/src/infrastructure/monitoring/",
    "@cache": "dist/src/infrastructure/cache/",
    "@storage": "dist/src/infrastructure/storage/",
    "@utils": "dist/src/core/utils/",
    "@errors": "dist/src/core/errors/",
    "@models": "dist/src/core/common/models/",
    "@helpers": "dist/src/core/common/helpers/",
    "@services": "dist/src/core/common/services/",
    "@constants": "dist/src/core/constants/",
    "@validators": "dist/src/interfaces/validators/",
    "@graphql": "dist/src/interfaces/graphql/",
    "@dtos": "dist/src/interfaces/dtos/",
    "@logs": "logs/"
  }
}
